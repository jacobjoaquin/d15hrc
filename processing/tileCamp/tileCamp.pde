

String data[] = {
  "0000000111110000000", 
  "0000111111111110000", 
  "0001111111111111000", 
  "0011111111111111100", 
  "0111111000001111110", 
  "0111100000000011110", 
  "0111000000000001110", 
  "1110000000000000111", 
  "1110000000000000111", 
  "1100000000000000011", 
  "1100000011100000011", 
  "1100000111110000011", 
  "0100000111110000010", 
  "0100000011100000110", 
  "0110000000000000110", 
  "0010000000000000100", 
  "0001000000000001000", 
  "0000100000000010000",
  "0000000001000000000"
};

void setup() {
  size(500, 500, P2D);
  noLoop();
  
  float w = float(width) / 20.0;
    
  fill(#F09827);  
  for (int y = 0; y < data.length; y++) {
    String row = data[y];
    float yPos = y * w + w * 0.5;
    for (int x = 0; x < row.length(); x++) {
      if (row.charAt(x) == '1') {
        rect(x * w + w * 0.5, yPos, w, w);
      }
    }
  }
}
